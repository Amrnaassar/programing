#include <iostream>
#include <algorithm>
#include <string>

using namespace std;

int main()
{
	
	int n;
	cin >> n;
	while (n--)
	{
		string s, s1="one",s2="two";
		int c = 0;
		cin >> s;

		if (s.size()== 5)
			cout << "3" << endl;
		else
		{
			for (int i = 0; i < s.size(); i++)
			{
				if (s[i] != s1[i])
					c++;
			}

			if (c <= 1)
				cout << "1" << endl;
			else
			{
				
				c = 0;
				for (int i = 0; i < s.size(); i++)
				{
					if (s[i] != s2[i])
						c++;
				}
				
				if (c <= 1)
					cout << "2" << endl;
			}

		}


	}



	//system("pause");
}
