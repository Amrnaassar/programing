
/*_*  I Can  &  I Will  * _ */

#include <iostream>
#include <string>
#include <algorithm>
#include <cmath>
#include <sstream>
#include <set>
#include <bitset>
#include <map>
#include <queue>
#include <math.h>
#include <deque>
#include <stack>
#include <vector>
#include <iomanip>
typedef long long ll;
typedef long double ld;

using namespace std;
void NasSaR()
{
	std::ios_base::sync_with_stdio(0);
	cin.tie(NULL);
	cout.tie(NULL);
}


int main()
{
	NasSaR();
	int t;
	while (cin >> t && t)
	{
		map<vector<int>, int>mp;

		int mx = 0;
		for (int j = 0; j < t; j++)
		{	
			vector<int>v(5);

			for (int i = 0; i < 5; i++)
				cin >> v[i];

			sort(v.begin(), v.end());

			mp[v]++;
			mx = max(mx, mp[v]);
		}

		int ans = 0;

		for (auto it:mp)
		{
			if (mx == it.second)
				ans+=it.second;
		}
		cout << ans << endl;
	}

	//system("pause");
	return 0;
}
