#include <iostream>
#include <string>
#include <algorithm>
#include <sstream>
#include <cmath>
#include <math.h>
#include <iomanip>

using namespace std;

int main()
{
	std::ios_base::sync_with_stdio(0);
	cin.tie(NULL);
	cout.tie(NULL);



	int t, a = 1;
	cin >> t;
	while (t--)
	{
		string s;
		int ans = 0;
		cin >> s;

		for (int i = 0; i < s.size(); i++)
		{
			if (s[i] == '.')
			{
				ans++;
				i += 2;
			}
		}

		cout << "Case " << a++ << ": " << ans << '\n';
	}

	system("pause");
	return 0;
}
