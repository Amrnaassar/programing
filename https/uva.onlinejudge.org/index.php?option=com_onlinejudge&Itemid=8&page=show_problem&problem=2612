
/*_*  I Can  &  I Will  * _ */

#include <iostream>
#include <string>
#include <algorithm>
#include <cmath>
#include <sstream>
#include <set>
#include <bitset>
#include <map>
#include <queue>
#include <math.h>
#include <deque>
#include <stack>
#include <vector>
#include <iomanip>
using namespace std;
void NasSaR()
{
	std::ios_base::sync_with_stdio(0);
	cin.tie(NULL);
	cout.tie(NULL);
}


int main()
{
	NasSaR();

	long long n;
	cin >> n;
	while (n--)
	{
		long long  x, y, z, a, b, c;
		bool f = 0;
		cin >> a >> b >> c;

		for (long long i = -100; i <= a; i++)
		{
			for (long long j = -100; j<= a; j++)
			{
				x = i;
				y = j;
				z = a - (i + j);
				if (x + y + z > a)
					break;
				if ((x + y + z == a) && (x*y*z == b) && (x*x + y * y + z * z == c))
				{
					if (x != y && y != z && x != z)
					{
						f = 1;
						break;
					}
				}
			}
			if (x + y + z > a)
				break;
			if (f)
				break;

		}

		if (f)
			cout << x << " " << y << " " << z << "\n";
		else
			cout << "No solution." << endl;
	}

	//system("pause");
	return 0;
}
