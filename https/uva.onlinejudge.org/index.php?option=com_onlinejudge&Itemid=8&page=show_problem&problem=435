#include <iostream>
#include <algorithm>
#include <cmath>
#include <string>
#include <vector>
using namespace std;

int main()
{

	string s;
	
	while (getline(cin, s))
	{
		int  c= 0;
		string x = "";
		for (int i = 0; i < s.size(); i++)
		{
			if ((s[i] >= 'A'&& s[i] <= 'Z') || (s[i] >= 'a' && s[i] <= 'z'))
			{
				x += s[i];
			}
			else
			{
				if (x != "")
					c++;
				x = "";
			}
		}
		cout << c << "\n";
		
	}
	
	//system("pause");
}
