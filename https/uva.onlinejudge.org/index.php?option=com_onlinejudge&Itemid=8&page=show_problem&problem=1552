
/*_*  I Can  &  I Will  *_*/

#include <iostream>
#include <string>
#include <algorithm>
#include <cmath>
#include <sstream>
#include <set>
#include <bitset>
#include <map>
#include <queue>
#include <deque>
#include <stack>
#include <vector>
#include <iomanip>
using namespace std;
void NasSaR()
{
	std::ios_base::sync_with_stdio(0);
	cin.tie(NULL);
	cout.tie(NULL);
}
int arr[100000];
int main()
{
	NasSaR();

	int n, q,a;
	cin >> n;

	for (int i = 0; i < n; i++)
		cin >> arr[i];
	sort(arr, arr + n);
	cin >> q;
	while (q--)
	{
		int x = -1, y = -1, l = 0, r = n - 1, mid;
		cin >> a;

		while (l <= r)
		{
			mid = (l + r) / 2;

			if (arr[mid] < a)
			{
				x = arr[mid];

				l = mid + 1;
			}
			else
				r = mid - 1;
			
		}
		l = 0, r = n - 1;

		while (l <= r)
		{
			mid = (l + r) / 2;

			if (arr[mid] > a)
			{
				y = arr[mid];

				r = mid - 1;
			}
			else
				l = mid + 1;

		}

		
		if (x != -1 && y != -1)
			cout << x << " " << y << endl;
		else if (x == -1 && y != -1)
			cout << "X" << " "<<y << endl;
		else if (x != -1 && y == -1)
			cout << x << " " << "X" << endl;
		else
			cout << "X" << " " << "X" << endl;
	}

	
	//system("pause");
	return 0;
}
