
/*_*  I Can  &  I Will  * _ */

#include <iostream>
#include <string>
#include <algorithm>
#include <cmath>
#include <sstream>
#include <set>
#include <bitset>
#include <map>
#include <queue>
#include <math.h>
#include <deque>
#include <stack>
#include <vector>
#include <iomanip>
using namespace std;
void NasSaR()
{
	std::ios_base::sync_with_stdio(0);
	cin.tie(NULL);
	cout.tie(NULL);
}


int main()
{
	NasSaR();

	int t,x=1;
	cin >> t;
	while (t--)
	{
		int n, p, q, sum = 0,c=0;
		cin >> n >> p >> q;
		vector<int>v(n);

		for (int i = 0; i < n; i++)
			cin >> v[i];
		sort(v.begin(), v.end());
		for (int i = 0; i < n; i++)
		{
			if (sum+v[i] <= q && c < p)
			{
				sum += v[i];
				c++;
			}
			else
				break;

		}

		cout << "Case "<< x++ << ": " <<c<< "\n";

	}
	

	//system("pause");
	return 0;
}
