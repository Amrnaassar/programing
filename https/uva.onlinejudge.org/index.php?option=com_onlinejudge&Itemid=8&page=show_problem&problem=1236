#include <iostream>
#include <string>
#include <algorithm>
#include <cmath>
#include <sstream>
#include <set>
#include <map>
#include <queue>
#include <deque>
#include <stack>
#include <vector>
#include <iomanip>
using namespace std;

int main()
{
	std::ios_base::sync_with_stdio(0);
	cin.tie(NULL);
	cout.tie(NULL);

	pair<string, int>arr[10000];
	int n, m;
	string s;
	cin >> n >> m;

	for (int i = 0; i < n; i++)
	{
		cin >> arr[i].first >> arr[i].second;
	}

	for (int j = 0; j < m; j++)
	{
		int sum = 0;
		while (cin>>s && s != ".")
		{
			for (int i = 0; i < n; i++)
			{
				if (s == arr[i].first)
					sum += arr[i].second;
			}

		}
		cout << sum << "\n";
	}

	//system("pause");
	return 0;
}
