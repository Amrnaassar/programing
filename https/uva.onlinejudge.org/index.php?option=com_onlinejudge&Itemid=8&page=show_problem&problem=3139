
/*_*  I Can  &  I Will  * _ */

#include <iostream>
#include <string>
#include <algorithm>
#include <cmath>
#include <sstream>
#include <set>
#include <bitset>
#include <map>
#include <queue>
#include <math.h>
#include <deque>
#include <stack>
#include <vector>
#include <iomanip>
using namespace std;
void NasSaR()
{
	std::ios_base::sync_with_stdio(0);
	cin.tie(NULL);
	cout.tie(NULL);
}


int main()
{
	NasSaR();
	string s;

	while (cin >> s)
	{
		string ans = "", x = "";
		bool t=false;
		for (int i = 0; i < s.size(); i++)
		{
			if (s[i] == '[')
			{
				ans = x + ans;
				x = "";
				t = true;
				
			}
			else if (s[i] == ']')
			{
				ans = x + ans;
				x = "";
				t = false;
			}
			else
			{
				if (s[i] == '['|| s[i]==']')
					continue;
				else if (t == true)
					x += s[i];
				else if(t==false)
					ans += s[i];

			}
		}
		ans=x+ans;
		cout << ans << "\n";
	}


	//system("pause");
	return 0;
}
