
/*_*  I Can  &  I Will  * _ */

#include <iostream>
#include <string>
#include <algorithm>
#include <cmath>
#include <sstream>
#include <set>
#include <bitset>
#include <map>
#include <queue>
#include <math.h>
#include <deque>
#include <stack>
#include <vector>
#include <iomanip>
using namespace std;
void NasSaR()
{
	std::ios_base::sync_with_stdio(0);
	cin.tie(NULL);
	cout.tie(NULL);
}


int main()
{
	NasSaR();

	int t;
	cin >> t;
	while (t--)
	{
		int n, ans = 0;
		cin >> n;
		vector<int>v(n);
		for (int i = 0; i < n; i++)
			cin >> v[i];


		for (int i = 0; i < n; i++)
		{
			for (int j = i + 1; j < n; j++)
			{
				if (v[i] > v[j])
					ans++;
			}
		}

		cout << "Optimal train swapping takes " << ans << " swaps.\n";
	}

	//system("pause");
	return 0;
}
