#include <iostream>
#include <algorithm>
#include <string>
using namespace std;

int main()
{
	string s,x;
	
	while (getline(cin, s))
	{
		int n=0, c=0;
		for (int i = 0; i < s.size(); i++)
		{
			if (s[i] == ' ' || i == s.size() - 1)
			{
				if (i == s.size() - 1)
					c++;

				x = "";
				x = s.substr(n, c);
				reverse(x.begin(), x.end());
				if (i == s.size() - 1)
					cout << x;
				else
				    cout << x << " ";

				n = i + 1;
				c = 0;
			}
			else
				c++;

		}
		cout << endl;
	}
	//system("pause");
	return 0;
}
