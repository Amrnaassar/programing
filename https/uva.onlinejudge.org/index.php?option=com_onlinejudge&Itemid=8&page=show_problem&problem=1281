#include <iostream>
#include <string>
#include <algorithm>
#include <cmath>

using namespace std;

int main()
{
	string a, b;
	while (cin >> b >> a)
	{	
		int x = 0, c = 0;

		for (int i = 0; i < b.size(); i++)
		{
			for (int j = x; j < a.size(); j++)
			{
				if (b[i] == a[j])
				{
					x = j + 1;

					c++;

					break;
				}
			}
		}

		if (c == b.size())
			cout << "Yes" << endl;
		else
			cout << "No" << endl;
	}
	//system("pause");
}
