#include <iostream>
#include <string>
#include <algorithm>
#include <cmath>
#include <sstream>
#include <set>
#include <map>
#include <queue>
#include <deque>
#include <stack>
#include <vector>
#include <iomanip>
using namespace std;
long long arr[1000000], cm[1000000];
int main()
{
	std::ios_base::sync_with_stdio(0);
	cin.tie(NULL);
	cout.tie(NULL);

	int n,q,l,r,c=0;
	cin >> n;
	for (int i = 0; i < n; i++)
		cin >> arr[i];

	cin >> q;

	for (int i = 0; i < q; i++)
	{
		cin >> l >> r;

		cm[l - 1]++;
		cm[r]--;
	}

	for (int i = 1; i < n; i++)
	{
		cm[i] += cm[i - 1];

		if (cm[i] == 0)
			c++;
	}

	if (cm[0] == 0)
		c++;
	cout << c << "\n";
	for (int i = 0; i < n; i++)
	{
		if (cm[i] == 0)
			cout << arr[i] << " ";
	}
	cout << "\n";
	//system("pause");
	return 0;
}
