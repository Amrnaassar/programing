#include <deque>
#include <stack>
#include <vector>
#include <iomanip>
typedef long long ll;
typedef long double ld;

using namespace std;
void Mekameleen()
{
	std::ios_base::sync_with_stdio(0);
	cin.tie(NULL);
	cout.tie(NULL);
}
ll s(ll x)
{
	ll sum = 0;

	while (x > 0)
	{
		sum += x % 10;
		x /= 10;
	}
	return sum;
}

int main()
{
	Mekameleen();
	ll n,x,a;
	cin >> n;

	if (0 > sqrt(n) - 1000)
		a = 1;
	else
		a = sqrt(n) - 1000;
	
	for (ll i = sqrt(n); i >= a; i--)
	{
		if (i*i + s(i)*i == n)
		{
			cout << i << endl;

			return 0;
		}
		
	}
		
	cout << -1;

	return 0;
}
