#include <iostream>
#include <cmath>
#include <algorithm>
#include <string>
using namespace std;
int main()
{
	char l;
	int a[26]{0}, n, x;
	bool ans = false;

	cin >> n;

	for(int i=0 ;i<n ;i++)
	{
		cin >> l;
		if (l >= 'a' && l <= 'z')
			l -= 32;

		x = l - 'A';
		
		a[x]++;
	}

	for (int i = 0; i < 26; i++)
	{
		if (a[i]==0)
		{
			ans = true;
		
		}
	}

	if (ans == true)
		cout << "NO" << endl;
	else
		cout << "YES" << endl;

	
}
